#include <stdlib.h>
void inicio (void);
void plano (void);
void menu (void);
void cadastrar (void);
//void localizar (void);
//void sair (void);
int main (void){
 inicio();
 plano();
 menu();
 cadastrar();
 //localizar();
 //sair();
return 0;
}
void inicio (void){
	printf("universidade ferderal do rio grande do norte\n");
     printf("Centro de Ensino Superior do Seridó\n");
     printf("Departamento de Computação e Tecnologia\n");
	printf("diciplina DCT1106-PROGRAMAÇÃO\n");
     printf("desenvolvedor- Francimar Alexandre de Oliveira Dantas\n");
	printf("\n");	
}
void plano (void){
     printf("O presente projeto tem como objetivo desenvolver um programa de computador que possibilite aos\n");
     printf("administradores dessas instituições controlar o fluxo, entradas e saídas de veículos em seu estabelecimento.\n");
	printf("\n");
}
void menu (void){
	printf("1-cadastrar vaiculo\n");
	printf("2-vargas desponiveis\n");
	printf("3-virsualizar carros\n");
	printf("4-sair");
	printf("\n");
}
//TELA DE CADASTRO
void cadastrar(void){
	char NOME,CPF,MODELO;
	printf("\t\tTELA DE CADASTRO\n");
	printf("\t\t=================\n");
	printf("DIGITE SEU CPF\n");
	scanf("%[^\n]", &CPF);
	printf("DIGITE SEU NOME\n");
	scanf("%s",&NOME);
  printf("DIGiTE O MODELO DO CARRO\n");
  scanf("%s",&MODELO);
}
//TELA LOCARLIZAR VEICULO
